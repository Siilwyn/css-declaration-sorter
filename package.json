{
  "name": "css-declaration-sorter",
  "version": "6.0.0",
  "description": "Sorts CSS declarations fast and automatically in a certain order.",
  "type": "module",
  "main": "./src/main.cjs",
  "exports": {
    "import": "./src/main.mjs",
    "require": "./src/main.cjs"
  },
  "files": [
    "src/main.mjs",
    "src/main.cjs",
    "src/shorthand-data.mjs",
    "orders"
  ],
  "scripts": {
    "build": "rollup src/main.mjs --format cjs --file src/main.cjs",
    "test": "node src/main.test.mjs",
    "test:ci": "npm test && npm run lint -- --max-warnings 0",
    "lint": "eslint src/*.mjs",
    "preversion": "npm test",
    "scrape": "node src/property-scraper.mjs",
    "prepack": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@mdn/browser-compat-data": "^2.0.3",
    "eslint": "^7.11.0",
    "postcss": "^8.1.1",
    "rollup": "^2.27.0",
    "tape": "^5.0.1"
  },
  "peerDependencies": {
    "postcss": "^8.0.9"
  },
  "engines": {
    "node": ">= 12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Siilwyn/css-declaration-sorter.git"
  },
  "author": "Selwyn <talk@selwyn.cc> (https://selwyn.cc/)",
  "license": "ISC",
  "keywords": [
    "postcss",
    "postcss-plugin",
    "css",
    "declaration",
    "sorter",
    "property",
    "order"
  ]
}
